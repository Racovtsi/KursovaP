//----------------------------------------------------------------------------
#include <vcl.h>
#include <vector>
#pragma hdrstop

#include "Unit1.h"
#include "Unit2.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"

TForm1 *Form1;
String form2open;
extern std::vector<int>winSpin;  //extern, сделать видимым всей программе

 //Функція вивода електронної формули атомів на Form2
void __fastcall TForm1::OpenForma2(wchar_t efa[255])
{
 Form1->Visible = false;
 Form2->Label1->Caption = "";
 Form2->Label1->Caption = Form2->Label1->Caption+efa;
 Form2->Show();
 }

__fastcall TForm1::TForm1(TComponent* Owner)
	: TForm(Owner)
{

}
//---------------------------------------------------------------------------
void __fastcall TForm1::Image1MouseDown(TObject *Sender, TMouseButton Button, TShiftState Shift,
		  int X, int Y)
{
OpenForma2(L"1s\u00b9");
const int mass[] = {2}; //ok
for(int i = 0; i<sizeof(mass)/sizeof(mass[0]);i++)
winSpin.push_back(mass[i]);
}
//---------------------------------------------------------------------------

